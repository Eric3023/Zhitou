page {
  background: white;
}

/* 投放天数 */
.container {
  display: flex;
  flex-direction: column;
}

.header {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  width: 100%;
  height: 300rpx;
  /* padding-left: 5%;
  padding-right: 5%; */
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url("data:image/jpg;base64,");
}

.remain {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #527300;
  text-align: center;
}

.remain_num {
  font-size: 38rpx;
  font-weight: 600;
}

.remain_title {
  margin-top: 10rpx;
  font-size: 30rpx;
}

.exposed {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #6b7300;
  text-align: center;
}

.exposed_num {
  margin-top: 10rpx;
  font-size: 30rpx;
  font-weight: 600;
}

.exposed_title {
  margin-top: 10rpx;
  font-size: 24rpx;
}

.body {
  margin: 50rpx;
}

.body .title_container {
  font-size: 24rpx;
  color: #000000;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.body .point {
  width: 18rpx;
  height: 18rpx;
  background-color: #57b77e;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 18rpx;
}

.body .img {
  margin-left: 80rpx;
  margin-top: 40rpx;
  margin-bottom: 18rpx;
  width: 490rpx;
  height: 132rpx;
}

.body .tail {
  height: fit-content;
  margin-top: 34rpx;
  margin-right: 20rpx;
  font-size: 24rpx;
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  position: relative;
  justify-content: space-between;
}

.body .tail .data {
  max-width: 380rpx;
  display: flex;
  flex-direction: column;
}

.monitor{
  max-width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}