page {
  width: 100%;
  height: 100%;
  background-color: fafafa;
}

.container {
  height: fit-content;
}

.items {
  margin-top: 25rpx;
  background-color: white;
  border-radius: 10rpx;
  box-shadow: 0 0 8rpx #dedede;
}

.item {
  display: flex;
  flex-direction: column;
}

.item_container {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 105rpx;
  font-size: 30rpx;
  color: #212121;
  position: relative;
}

.item_container text {
  margin-left: 54rpx;
}

.line {
  height: 2rpx;
  width: 700rpx;
  margin-left: 25rpx;
  background-color: #ebebeb;
}

.item_container .enter {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 50rpx;
}

.loginout {
  margin-top: 36rpx;
  width: 100%;
  height: 120rpx;
  font-size: 36rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: white;
  border-radius: 10rpx;
  box-shadow: 0 0 8rpx #dedede;
  color: #f58371;
}

button {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border: none !important;
  z-index: 9;
}