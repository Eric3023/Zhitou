page{ width: 100%; height:100%; }

/* 搜索返回地址 */
.list{
  /* position: absolute; */
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
  overflow-y: scroll;
  background: #fff;
  z-index: 88;
  /* padding: 20rpx; */
  margin: 80rpx 0 0 0;
}
.list-li{
  width: 100%;
  height: 130rpx;
  line-height: 130rpx;
  /* position: relative; */
  /* padding-left: 76rpx; */
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
  margin: auto 0;
}
.list-img{
  display: inline;
  width: 20rpx;
  
  height: 20rpx;
  float: left;
  /* position: absolute; */
  /* left: 10rpx;
  top: 50%;
  transform: translateY(-50%); */
  margin:60rpx 0 30rpx 30rpx;
}
.list-img image{
  display: inline;
  width: 20rpx;
}
.list-r{
  float: left;
  width: 80%;
  margin-top: 12rpx;
  margin-left: 30rpx;
  margin-bottom: 12rpx;
  
}
.list-p{
  font-size: 26rpx;
  color: #434343;
  line-height: 50rpx;
  overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.list-span{
  font-size: 26rpx;
  color: #7070706b;
  line-height: 30rpx;
  margin-top: 10rpx;
  overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}