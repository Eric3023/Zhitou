page {
  background: white;
}

/* 投放天数 */
.container {
  display: flex;
  flex-direction: column;
}

.header {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 300rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url("data:image/jpg;base64,");
}

.remain {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #527300;
  margin-left: 172rpx;
}

.remain_num {
  font-size: 38rpx;
  font-weight: 600;
}

.remain_title {
  margin-top: 10rpx;
  font-size: 30rpx;
}

.exposed {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #6b7300;
  margin-left: 86rpx;
}

.exposed_num {
  margin-top: 10rpx;
  font-size: 30rpx;
  font-weight: 600;
}

.exposed_title {
  margin-top: 10rpx;
  font-size: 24rpx;
}

.body{
  display: flex;
  flex-direction: column;
  width: 708rpx;
  height: 420rpx;
  margin: 42rpx 21rpx 0 21rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
}

.body text{
  margin-left: 24rpx;
  font-size: 28rpx;
}

.body .tag{
  margin-top: 20rpx;
  color: #7c7b7b;
}

.body .num{
  margin-top: 4rpx;
  color: #000000;
}

.detail_container {
  margin: 42rpx 28rpx 0rpx ;
  box-shadow: 0rpx 0rpx 10rpx #dedede;
}